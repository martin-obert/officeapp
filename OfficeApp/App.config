  </configSections>
  <ExcelFormattersSection>
    

    <!--Pravidlo, kterým označíš klíčové slovo/slova a řekneš čím se mají nahradit-->
    <formatter keyword="MERKMAL" replacement="Merkmal" />
    

  </ExcelFormattersSection>
  <appSettings>

    <!--Cesta k souboru který chceš měnit-->
    <add key="filepath" value="C:\\Data\\L550MY20_HL_B_SC_DP_DRL_RHS_001.xls" />
    

    <!--Excelovská oblast, která se bude procházet a nahrazovat se v ní texty-->
